<p style="color: green"><%= notice %></p>
<h1>Diaries</h1>
<table id="diaries" class="table table-striped">
  <thead>
    <tr>
      <th scope="col">
        Name
      </th>
      <th scope="col">
        Actions
      </th>
    </tr>
  </thead>
  <% @diaries.each do |diary| %>
    <%= render diary %>
  <% end %>
</table>
<%= link_to "New diary", new_diary_path, class: "btn btn-primary" %>
